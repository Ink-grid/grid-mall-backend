{
	"name": "gridmallvs",
	"version": "1.0.0",
	"description": "grimall is the ultimate backend solution",
	"main": "src/server.js",
	"scripts": {
		"setup": "npm install && npm update",
		"tsc": "tsc",
		"build": "npm run tsc",
		"start": "ts-node-dev --transpileOnly ./src/server.ts",
		"dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
		"prod": "tsc && node ./build/src/server.js"
	},
	"keywords": [
		"express",
		"backend",
		"nevado_back_new"
	],
	"license": "ARTISTIC-2.0",
	"resolutions": {
		"apollo-link": "^1.2.14"
	},
	"dependencies": {
		"@types/express": "^4.17.6",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"express": "^4.17.3",
		"express-custom-error": "^1.8.4",
		"express-graphql": "^0.9.0",
		"firebase-admin": "^8.12.1",
		"graphql": "^14.6.0",
		"graphql-tools": "^4.0.8",
		"graphql-type-json": "^0.3.2",
		"has-keys": "^0.1.0",
		"helmet": "^3.21.2",
		"http-status": "^1.3.2",
		"mandatoryenv": "^1.1.1",
		"module-alias": "^2.2.2",
		"morgan": "^1.9.1",
		"socket.io": "^2.3.0",
		"stripe": "^8.56.0",
		"ts-node-dev": "^1.0.0-pre.43",
		"typescript": "^3.9.2"
	},
	"_moduleAliases": {
		"@root": "src",
		"@controllers": "src/controllers",
		"@models": "src/models",
		"@util": "src/util",
		"@routes": "src/routes"
	}
}
